
@{
    ViewBag.Title = "Add";
    Layout = "~/Areas/SysAdmin/Views/Shared/_AdminLayout.cshtml";
}

    
        <div class="row">
            <div class="col-lg-8 offset-lg-2">
                <h4 class="page-title">Sıkça Sorulan Sorular</h4>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-8 offset-lg-2">
                <form action="/SysAdmin/QA/Add" role="form" enctype="multipart/form-data" method="post">
                    <div class="form-group">
                        <label for="Content">Soru</label>
                        <input name="Content" placeholder="Soru yazınız.." class="form-control" type="text">
                    </div>
                    <div class="form-group">
                        <label for="AnswerContent" >Cevap</label>
                        <textarea name="AnswerContent" cols="30" rows="4" class="form-control"></textarea>
                    </div>
                    @*<div class="form-group">
                        <input name="UserID" value="@Session["ID"]" />
                    </div>*@
                    <div class="m-t-20 text-center">
                        <button class="btn btn-primary submit-btn">Ekle</button>
                    </div>
                </form>
            </div>
        </div>
    

